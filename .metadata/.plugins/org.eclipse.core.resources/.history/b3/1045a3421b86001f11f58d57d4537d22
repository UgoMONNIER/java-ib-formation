package first_project;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		System.out.println("helo");
		int quantityStorage = new Scanner(System.in).nextInt();
		int quantityStorageInt = quantityStorage;
		if(quantityStorageInt > 100) {
			System.out.println("No need to buy");
		}else {
			System.out.println("Need to buy");
		}

	}

}
