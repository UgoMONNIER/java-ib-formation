public class Main {

	public static void main(String[] args) {
		System.out.println("helo");
		Scanner quantityStorage = new Scanner(System.in);
		if(quantityStorage > 100) {
			System.out.println("No need to buy");
		}else {
			System.out.println("Need to buy");
		}

	}

}
